{
  "name": "@konsumation/frontend-svelte",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "description": "svelte web frontend for konsum",
  "keywords": [
    "konsum",
    "svelte",
    "vite",
    "web"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    },
    {
      "name": "Konstantin Karpman"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "prepare": "vite build",
    "startkonsum": "node_modules/konsum/src/konsum-cli.mjs -c tests/config start",
    "startfrontend": "npm run dev",
    "start": "vite",
    "test": "npm run test:ava && npm run test:cafe",
    "test:cafe": "testcafe $BROWSER:headless tests/cafe/*.js -s build/test --app-init-delay 7000 --app \"vite\"",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cafeHead": "testcafe chrome tests/cafe/deleteValues.js -s build/test",
    "dev": "vite",
    "pkg": "npm-pkgbuild --verbose -Dversion=0.0.0",
    "serve": "vite preview"
  },
  "dependencies": {
    "imask": "^6.4.2",
    "layercake": "^6.0.2",
    "mf-styling": "arlac77/mf-styling",
    "streamsaver": "^2.0.6",
    "svelte-command": "^1.1.6",
    "svelte-common": "^4.4.29",
    "svelte-guard-history-router": "^4.5.2",
    "svelte-session-manager": "^2.0.10",
    "the-new-css-reset": "^1.4.7"
  },
  "devDependencies": {
    "@konsumation/konsum": "github:konsumation/konsum",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.40",
    "ava": "^4.1.0",
    "konsum": "github:konsumation/konsum",
    "npm-pkgbuild": "^8.1.1",
    "semantic-release": "^19.0.2",
    "svelte": "^3.46.5",
    "testcafe": "^1.18.4",
    "vite": "^2.9.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/konsumation/konsum-frontend-svelte.git"
  },
  "bugs": {
    "url": "https://github.com/konsumation/konsum-frontend-svelte/issues"
  },
  "homepage": "https://github.com/konsumation/konsum-frontend-svelte#readme",
  "config": {
    "api": "/services/konsum/api",
    "base": "/services/konsum",
    "title": "Konsum"
  },
  "pkg": {
    "content": {
      "${installdir}/": {
        "base": "build"
      },
      "/etc/nginx/sites/common/${name}.conf": "pkg/nginx.conf"
    },
    "output": {
      "arch": {}
    },
    "hooks": "pkg/hooks.sh",
    "groups": "web",
    "depends": {
      "nginx-mainline": ">=1.21.4",
      "konsum": ">=5.0.39"
    },
    "installdir": "/services/konsum/frontend",
    "name": "konsum-frontend-svelte"
  },
  "release": {
    "plugins": [
      [
        "@semantic-release/exec",
        {
          "publishCmd": "npx npm-pkgbuild"
        }
      ],
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "template": {
    "properties": {
      "testcafe": {
        "options": "--app-init-delay 7000"
      }
    },
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-ava",
      "arlac77/template-pacman",
      "arlac77/template-svelte-app#next",
      "arlac77/template-testcafe",
      "konsumation/template"
    ]
  }
}
