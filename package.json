{
  "name": "@konsumation/frontend-svelte",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "description": "svelte web frontend for konsum",
  "keywords": [
    "konsum",
    "svelte",
    "vite",
    "web"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    },
    {
      "name": "Konstantin Karpman"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "prepare": "vite build",
    "startkonsum": "node_modules/konsum/src/konsum-cli.mjs -c tests/config start",
    "start": "vite",
    "test": "npm run test:ava && npm run test:cafe",
    "test:cafe": "testcafe $BROWSER:headless tests/cafe/*.js -s build/test --app-init-delay 7000 --app \"vite preview\"",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "lint": "npm run lint:css",
    "lint:css": "stylelint ./src/*.css",
    "cafeHead": "testcafe chrome tests/cafe/deleteValues.js -s build/test",
    "preview": "vite preview"
  },
  "dependencies": {
    "imask": "^6.4.2",
    "layercake": "^7.0.0",
    "mf-hosting": "^1.7.3",
    "mf-styling": "^1.7.14",
    "streamsaver": "^2.0.6",
    "svelte-command": "^1.1.24",
    "svelte-common": "^4.19.6",
    "svelte-entitlement": "^1.2.30",
    "svelte-guard-history-router": "^5.4.2",
    "svelte-log-view": "^4.2.14",
    "svelte-session-manager": "^2.0.10",
    "the-new-css-reset": "^1.7.2"
  },
  "devDependencies": {
    "@konsumation/konsum": "^6.1.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@sveltejs/vite-plugin-svelte": "^1.0.5",
    "ava": "^4.3.3",
    "npm-pkgbuild": "^10.14.8",
    "semantic-release": "^19.0.5",
    "stylelint": "^14.11.0",
    "stylelint-config-standard": "^28.0.0",
    "svelte": "^3.50.0",
    "testcafe": "^2.0.0",
    "vite": "^3.1.0"
  },
  "optionalDependencies": {
    "mf-hosting": "^1.7.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/konsumation/konsum-frontend-svelte.git"
  },
  "bugs": {
    "url": "https://github.com/konsumation/konsum-frontend-svelte/issues"
  },
  "homepage": "https://github.com/konsumation/konsum-frontend-svelte#readme",
  "config": {
    "base": "/services/frontend-svelte",
    "title": "Konsum"
  },
  "pkgbuild": {
    "content": {
      "${install.dir}": "build/",
      "${nginx.sites.dir}${name}.conf": "pkg/nginx.conf"
    },
    "hooks": "pkg/hooks.sh",
    "depends": {
      "konsum": ">=6.1.4"
    },
    "http.api.path": "${http.path}/api",
    "http.path": "${http.base.path}/konsum",
    "install.dir": "${install.base.dir}konsum/frontend/",
    "install.sub.dir": "/frontend/",
    "name": "konsum-frontend-svelte"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      [
        "@semantic-release/exec",
        {
          "publishCmd": "npx npm-pkgbuild --available --continue --verbose"
        }
      ],
      "@semantic-release/github"
    ]
  },
  "template": {
    "properties": {
      "testcafe": {
        "options": "--app-init-delay 7000"
      }
    },
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-ava",
      "arlac77/template-pacman",
      "arlac77/template-svelte-app#next",
      "arlac77/template-testcafe",
      "konsumation/template"
    ]
  }
}
