{
  "name": "konsum-frontend-svelte",
  "version": "0.0.0-semantic-release",
  "description": "svelte web frontend for konsum",
  "keywords": [
    "svelte",
    "web"
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "prepare": "rollup -c",
    "start": "rollup -c -w",
    "package": "npm-pkgbuild pkgbuild pacman makepkg"
  },
  "dependencies": {
    "mf-styling": "arlac77/mf-styling",
    "svelte-common": "^2.0.2",
    "svelte-guard-history-router": "^1.1.17",
    "svelte-session-manager": "^1.0.10",
    "svelte-time-series": "arlac77/svelte-time-series"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "npm-pkgbuild": "^6.11.17",
    "postcss-import": "^12.0.1",
    "rollup": "^2.7.3",
    "rollup-plugin-consts": "^1.0.1",
    "rollup-plugin-dev": "^1.1.2",
    "rollup-plugin-postcss": "^3.1.1",
    "rollup-plugin-svelte": "^5.2.1",
    "rollup-plugin-terser": "^5.3.0",
    "semantic-release": "^17.0.7",
    "svelte": "^3.21.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/konsum-frontend-svelte.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/konsum-frontend-svelte/issues"
  },
  "homepage": "https://github.com/arlac77/konsum-frontend-svelte#readme",
  "config": {
    "api": "/services/konsum/api",
    "base": "/services/konsum",
    "proxyTarget": "https://mfelten.dynv6.net"
  },
  "pacman": {
    "content": {
      "${installdir}/": {
        "base": "public"
      },
      "/etc/nginx/config.d/${name}.conf": "pacman/nginx.conf"
    },
    "depends": {
      "nginx-mainline": ">=1.17.8",
      "konsum": ">=1.34.28"
    },
    "groups": "home automation",
    "hooks": "pacman/hooks.sh",
    "installdir": "/services/konsum/frontend"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/exec",
        {
          "publishCmd": "npx npm-pkgbuild --pkgver ${nextRelease.version} pkgbuild pacman makepkg"
        }
      ]
    ]
  },
  "template": {
    "inheritFrom": [
      "arlac77/template-svelte-app",
      "arlac77/template-github"
    ]
  }
}
